{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/server.ts": {
      "memory": 512,
      "maxDuration": 10,
      "includeFiles": "dist/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/server.ts"
    }
  ]
}


